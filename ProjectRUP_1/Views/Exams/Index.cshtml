        <th>
            @Html.DisplayNameFor(model => model.Speciality)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ComputerNeeded)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Surveillant)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpectedStudents)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResultIsDecimal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QuarterId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinutesDuration)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
