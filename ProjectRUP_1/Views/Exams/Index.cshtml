                @Html.DisplayFor(modelItem => item.ResultIsDecimal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuarterId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MinutesDuration)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
